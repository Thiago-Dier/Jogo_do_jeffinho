   escreva("@+  ......=%@*:.             ....         =@\n")
   escreva("@+ .....+@@@@@@%:....       ..............=@\n")
   escreva("@+  ....@@@@@@@@= ..        .....  .......=@\n")
   escreva("@+     .#@@@@@@@-                         =@\n")
   escreva("@+      .+@@@@#:.                         =@\n")
   escreva("@+                                        =@\n")
   escreva("@+              ..... .  ..              .=@\n")
   escreva("@=.......      ..................       ..=@\n")
   escreva("@+......        ................        ..=@\n")
   escreva("@+                                        =@\n")
   escreva("@+                                        =@\n")
   escreva("@+                         .:*@@@@+.      =@\n")
   escreva("@+ ..................      -@@@@@@@#......=@\n")
   escreva("@+ ..................      =@@@@@@@@:.....=@\n")
   escreva("@+   ..                    :%@@@@@@+.     =@\n")
   escreva("@+                          .:*@%=..      =@\n")
   escreva("@*                                        +@\n")
   escreva("@@:                                      :@@\n")
   escreva(" @@#:....      ..................      :#@%-\n")
   escreva("   @@@@@@@@@@@@@@%@%@@@@@@@@@@@@@@@@@@@@@: .\n")
   u.aguarde(500)
  }

